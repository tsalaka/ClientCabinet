@model IEnumerable<RC.Content.Core.Entities.PublicationEntity>

@{ Html.RenderAction("Index", "Pager", new { currentPage=ViewBag.CurrentPage,itemsCount = ViewBag.ItemsCount, rowsCount = ViewBag.RowsCount, action = "Index", controller = "PressRelease" }); }


<table cellpadding="0" cellspacing="0" width="100%" class="publications">
@foreach(var item in Model)
{
	<tr>
		<td>
			@Html.ActionLink(item.Title, "PublicationGet", new { id = item.ID })
		</td>
	</tr>
}
	</table>