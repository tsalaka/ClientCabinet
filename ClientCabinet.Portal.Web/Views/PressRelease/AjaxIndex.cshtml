@model IEnumerable<RC.Content.Core.Entities.PublicationEntity>

<div id="pressReleaseDiv">
	
	@{ Html.RenderAction("AjaxIndex", "Pager", new { currentPage = ViewBag.CurrentPage, itemsCount = ViewBag.ItemsCount, rowsCount = ViewBag.RowsCount, action = "AjaxPage", controller = "PressRelease", updateTargetId = "pressReleaseDiv"}); }

	<table cellpadding="0" cellspacing="0" width="100%" class="publications">
		@foreach(var item in Model)
		{
			<tr>
				<td>
					@Html.ActionLink(item.Title, "PublicationGet", new { id = item.ID, page = ViewBag.CurrentPage })
				</td>
			</tr>
		}
	</table>
</div>